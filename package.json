{
  "name": "openclaw-gmail",
  "version": "0.1.0",
  "type": "module",
  "description": "OpenClaw Gmail channel plugin (gog) \u2014 per-thread sessions with Pub/Sub push",
  "dependencies": {
    "email-reply-parser": "^2.3.5",
    "google-auth-library": "^9.15.0"
  },
  "main": "index.ts",
  "license": "MIT",
  "author": "Cam Marsollier",
  "repository": {
    "type": "git",
    "url": "https://github.com/copperdogma/openclaw-gmail.git"
  },
  "keywords": [
    "openclaw",
    "gmail",
    "email",
    "channel",
    "plugin",
    "gog"
  ],
  "files": [
    "index.ts",
    "src",
    "openclaw.plugin.json",
    "README.md",
    "CHANGELOG.md",
    "PUBLISHING.md"
  ],
  "peerDependencies": {
    "openclaw": ">=2026.1.0"
  },
  "devDependencies": {
    "openclaw": ">=2026.1.0",
    "typescript": "^5.5.4",
    "jiti": "^1.21.0"
  },
  "scripts": {
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node --test tests/*.test.mjs"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "openclaw-gmail",
      "label": "Gmail",
      "selectionLabel": "Gmail (gog)",
      "docsPath": "/channels/gmail",
      "docsLabel": "gmail",
      "blurb": "Gmail channel (gog) \u2014 per-thread sessions.",
      "order": 92,
      "quickstartAllowFrom": true,
      "npmSpec": "openclaw-gmail",
      "localPath": "openclaw-gmail"
    },
    "install": {
      "npmSpec": "openclaw-gmail",
      "localPath": ".",
      "defaultChoice": "local"
    }
  }
}
